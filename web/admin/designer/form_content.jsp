
<div class="p_row">
    <div class="col-content">
        <p class="col-heading">Introduction </p>
        <label for="first_name">First Name </label>
        <input type="hidden" id="desginer_id"  name="desginer_id" value="0">
        <input type="text" id="first_name"  name="first_name" placeholder="John">
        <label for="surname">Surname </label>
        <input type="text" id="surname"  name="surname" placeholder="Doe">
        <label for="type">Type </label>
        <input type="text" id="type"  name="type" placeholder="Animation">
        <label for="msg">Message</label>
        <input type="text" id="msg" name="msg" placeholder="Designer message">
        <label for="phone">Phone Number</label>
        <input type="tel" id="phone"  name="phone" placeholder="01723489245">

    </div>
    <div class="col-content">
        <p class="col-heading">Social Media </p>
        <label for="email">Email </label>
        <input type="email" id="email" name="email" placeholder="xxx@gmail.com">
        <label for="linkedin">Linkedin </label>
        <input type="url" id="linkedin"  name="linkedin" placeholder="https://linkedin.com/in/xxx">
        <label for="facebook">Facebook </label>
        <input type="url" id="facebook"  name="facebook" placeholder="https://facebook.com/xxx">
        <label for="picture_url">Picture </label>
        <input type="file" id="picture_url" form="picture_upload_form"  name="picture_url" size="50" />

    </div>

</div>

